{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.png", "use": "@vercel/static" },
    { "src": "*.jpg", "use": "@vercel/static" },
    { "src": "*.jpeg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/([^/]+)\\.html", "dest": "/$1.html" },
    { "src": "/([^/]+)\\.png", "dest": "/$1.png" },
    { "src": "/([^/]+)\\.jpg", "dest": "/$1.jpg" },
    { "src": "/([^/]+)\\.mp4", "dest": "/$1.mp4" },
    { "src": "/.*", "dest": "/index.js" }
  ]
}
